<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sfc2">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SFC2">

        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>

        <activity
            android:name=".SplashScreen2"
            android:exported="true"
            android:theme="@style/SplashScreen2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Branches"
            android:label="Branches">
        </activity>
        <activity
            android:name=".Menu2"
            android:label="Menu">
        </activity>
        <activity
            android:name=".About"
            android:label="About">
        </activity>
        <activity
            android:name=".NavBar"
            android:label="NavBar">
        </activity>
        <activity
            android:name=".About2"
            android:label="About2">
        </activity>
        <activity
            android:name=".Booking"
            android:label="Booking">
        </activity>
        <activity
            android:name=".About3"
            android:label="About3">
        </activity>
    </application>

</manifest>